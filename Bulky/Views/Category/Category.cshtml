@model List<Category>;
<h1>Category</h1>

<table>
    <tr>
        <th>
            <tr>Name</tr>
            <tr>Display Order</tr>
        </th>
    </tr>
    @foreach (var category in Model.OrderBy(x => x.DisplayOrder))
    {
        <tr>
            <td>@category.CategoryName</td>
            <td>@category.DisplayOrder</td>
        </tr>
    }
</table>